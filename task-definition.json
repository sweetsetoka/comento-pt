{
  "executionRoleArn": "arn:aws:iam::216066129448:role/ecsTaskExecutionRole",
  "taskRoleArn": "",
  "containerDefinitions": [
    {
      "image": "test",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/comento-pt-nestjs-rk",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "cpu": 0,
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "ulimits": [
        {
          "name": "nofile",
          "softLimit": 65535,
          "hardLimit": 65535
        }
      ],
      "name": "nestjs",
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory" : "512",
  "family": "comento-pt-nestjs-rk"
}